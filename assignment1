## Question 13 ################################################
###############################################################

cat /home/unixtool/data/restaurants/all_grades.csv | cut -d, -f 2 | sort -f | uniq -i -c | sort -n -r | head -3

###############################################################
## Question 14 ################################################
###############################################################

cat /home/unixtool/data/restaurants/all_grades.csv | cut -d, -f 2,3 | sort | uniq | cut -d, -f 1 | uniq -d

###############################################################
## Question 15 ################################################
###############################################################

cat /home/unixtool/data/restaurants/all_grades.csv | cut -d, -f 6 | sort -n | uniq -c | sort -n -r | head -1 | tr -s " " | cut -d " " -f 3

###############################################################
## Question 16 ################################################
###############################################################

cat /home/unixtool/data/restaurants/all_grades.csv | cut -d, -f 2 | tr -s " " | tr " " "\n" | sort | uniq -c | sort -n | tail -1 | tr -s " " | cut -d " " -f 3
