## Question 9 #################################################
###############################################################

cat /home/unixtool/data/restaurants/all_grades.csv | sort -t, -n -k 9 | cut -d, -f 9 | sort | uniq -c | tr -s " " | cut -d " " -f 2,3

###############################################################
## Question 10 ################################################
###############################################################

cat /home/unixtool/data/restaurants/all_grades.csv | sort -t, -k 7 | tail -10000 | sort -t, -k 9 | cut -d, -f 9 | uniq -c

